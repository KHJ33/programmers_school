    "def solution(n):\n",
    "    answer = []\n",
    "    \n",
    "    while n != 1 :\n",
    "        \n",
    "        for i in range(2, n+1) :\n",
    "            if n % i == 0 :\n",
    "                n = n // i\n",
    "                answer.append(i)\n",
    "                break\n",
    "    \n",
    "    return list(set(answer))"
   ]
