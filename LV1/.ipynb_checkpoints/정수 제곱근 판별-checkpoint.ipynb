    "def solution(n):\n",
    "    answer = 0\n",
    "    \n",
    "    # 입력한 수를 루트를 씌워서 계산 진행\n",
    "    if (n ** (1/2)) * int(n ** (1/2)) == n :\n",
    "        answer = int(n ** (1/2)) + 1\n",
    "        answer = answer ** 2\n",
    "    \n",
    "    else :\n",
    "        answer = -1\n",
    "        \n",
    "    return answer"
